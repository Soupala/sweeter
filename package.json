{
  "name": "sweeter",
  "version": "0.0.40",
  "description": "CLI tool for concurrent JavaScript package development.",
  "files": ["lib"],
  "bin": {
    "sweeter": "./lib/sweeter.js"
  },
  "scripts": {
    "build": "nwb build-web-module && npm install -g && npm install -g",
    "clean": "nwb clean-module",
