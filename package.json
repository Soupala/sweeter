  "devDependencies": {
    "nwb": "0.16.3"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "commander": "^2.11.0",
    "fs-extra": "^3.0.1",
    "homedir": "^0.6.0",
    "mkdirp": "^0.5.1",
    "shelljs": "^0.7.8",
    "touch": "^3.1.0",
    "update-notifier": "^2.2.0"
  }
}
